{
  "name": "golden-layout-react-redux",
  "version": "1.0.0",
  "description": "example showing how to use react-redux with golden-layout",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run dev",
    "build": "chmod +x changeProduct.sh && ./changeProduct.sh && chmod +x build.sh && ./build.sh && webpack",
    "build-prod": "chmod +x changeProduct.sh && ./changeProduct.sh && chmod +x build.sh && ./build.sh && webpack",
    "electron": "electron .",
    "dev": "webpack-dev-server",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --app-bundle-id=com.quantedge.equixnext --icon=./resource/equix.icns --prune=true --out=release-builds",
    "package-window": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=./resource/equix.icon --prune=true --out=release-builds --version-string.CompanyName=QuantEdge --version-string.FileDescription=QuantEdge --version-string.ProductName=\"Equix\"",
    "package-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --prune=true --out=release-builds",
    "package-all": "npm run package-mac && npm run package-window && npm run package-linux",
    "test-mocha": "./node_modules/.bin/mocha"
  },
  "keywords": [
    "react",
    "redux",
    "react-redux",
    "golden-layout"
  ],
